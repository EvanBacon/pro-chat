{
  "name": "EXGL-CPP",
  "version": "1.1.0",
  "summary": "C++ bindings for WebGL 2.0 used in Expo GL module",
  "description": "C++ bindings for WebGL 2.0 used in Expo GL module",
  "license": "MIT",
  "authors": "650 Industries, Inc.",
  "homepage": "https://docs.expo.io/",
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/expo/expo.git"
  },
  "source_files": "**/*.{h,c,cpp,mm}",
  "preserve_paths": "**/*.{h,c,cpp,mm}",
  "exclude_files": "UEXGL.*",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "CLANG_WARN_COMMA": "NO",
    "CLANG_WARN_UNGUARDED_AVAILABILITY": "NO"
  },
  "subspecs": [
    {
      "name": "UEXGL",
      "source_files": "UEXGL.*",
      "compiler_flags": "-x objective-c++"
    }
  ]
}
