{
  "name": "ExpoKit",
  "version": "2.9.0",
  "summary": "ExpoKit",
  "description": "ExpoKit allows native projects to integrate with the Expo SDK.",
  "homepage": "http://docs.expo.io",
  "license": "MIT",
  "authors": "650 Industries, Inc.",
  "requires_arc": true,
  "platforms": {
    "ios": "10.0"
  },
  "default_subspecs": "Core",
  "source": {
    "git": "http://github.com/expo/expo.git"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "ios/Exponent/**/*.{h,m}",
        "template-files/keys.json"
      ],
      "preserve_paths": "ios/Exponent/**/*.{h,m}",
      "exclude_files": [
        "ios/Exponent/Supporting/**",
        "ios/Exponent/Versioned/Optional/**/*.{h,m}"
      ],
      "dependencies": {
        "Amplitude-iOS": [
          "~> 3.8"
        ],
        "Analytics": [
          "~> 3.5"
        ],
        "AppAuth": [
          "~> 0.4"
        ],
        "CocoaLumberjack": [
          "~> 3.2.1"
        ],
        "Crashlytics": [
          "~> 3.8"
        ],
        "FBAudienceNetwork": [
          "4.99.0"
        ],
        "FBSDKCoreKit": [
          "4.37.0"
        ],
        "FBSDKLoginKit": [
          "4.37.0"
        ],
        "FBSDKShareKit": [
          "4.37.0"
        ],
        "Fabric": [
          "~> 1.6"
        ],
        "GoogleSignIn": [
          "~> 4.1"
        ],
        "GoogleMaps": [
          "~> 2.5.0"
        ],
        "Google-Maps-iOS-Utils": [
          "~> 2.1.0"
        ],
        "lottie-ios": [
          "~> 2.5.0"
        ],
        "JKBigInteger2": [
          "0.0.5"
        ],
        "Branch": [
          "~> 0.24.2"
        ],
        "Google-Mobile-Ads-SDK": [
          "~> 7.22.0"
        ],
        "React": [

        ],
        "EXCore": [

        ],
        "EXReactNativeAdapter": [

        ],
        "EXSensorsInterface": [

        ],
        "EXFileSystemInterface": [

        ],
        "EXPermissionsInterface": [

        ],
        "EXCameraInterface": [

        ],
        "EXConstantsInterface": [

        ]
      }
    },
    {
      "name": "Payments",
      "dependencies": {
        "ExpoKit/Core": [

        ],
        "Stripe": [
          "~> 10.1.0"
        ]
      },
      "source_files": "ios/Exponent/Versioned/Optional/Payments/*.{h,m}"
    },
    {
      "name": "AR",
      "dependencies": {
        "ExpoKit/Core": [

        ]
      },
      "source_files": "ios/Exponent/Versioned/Optional/ARKit/**"
    },
    {
      "name": "FaceDetector",
      "dependencies": {
        "EXFaceDetector": [

        ]
      }
    }
  ]
}
